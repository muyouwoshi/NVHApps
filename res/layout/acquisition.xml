        android:gravity="center"
       	android:textColor="#465468"
       	android:textSize="12sp"
       	android:entries="@array/AcquiFreq_spinner"
        android:spinnerMode="dropdown" />
   <!--  @array/sampling_rate_v -->
    <TextView
        android:id="@+id/sampling_rate_v_Hz"
        android:layout_width="17dp"
        android:layout_height="35dp"
        android:gravity="center_vertical"
        android:layout_marginRight="2dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/sampling_rate_n_Line"
        android:textSize="12sp"
        android:text="@string/Hz"
        android:textColor="?attr/all_title_text"/>  
    <TextView
        android:id="@+id/sampling_rate_v_Line"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#3C404C"
        android:layout_below="@+id/sampling_rate_v"/>
