<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    			android:layout_width="match_parent"
    			android:layout_height="wrap_content">
    	   		<RelativeLayout 
        				android:id="@+id/rl_dialog_content"
        				android:layout_width="match_parent"
        				android:layout_height="185dp"
        				android:layout_marginLeft="20dp"
        				android:layout_marginRight="20dp"
        				android:layout_centerInParent="true"
        				android:background="@drawable/welcome_title_corners_bg">
            		<TextView
            			android:id="@+id/dialog_title"
            			android:layout_width="400dp"
            			android:layout_height="50dp"
            			android:gravity="center_vertical|center_horizontal"
            			android:text="@string/equipment_dialog_title"
            			android:textColor="#191970"
            			android:textSize="16sp" />
            		<LinearLayout
                		android:layout_width="match_parent"
                		android:layout_height="140dp"
                		android:layout_alignParentBottom="true"
                		android:background="@drawable/corners_bg2"
                		android:orientation="vertical" >
                	<LinearLayout
                    	android:layout_width="match_parent"
                    	android:layout_height="match_parent"
                    	android:layout_weight="2"
                    	android:gravity="center_vertical|center_horizontal"
                    	android:orientation="horizontal" >
                    <Button
                        android:id="@+id/dialog_ok"
                        android:layout_width="20dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="20dip"
                        android:layout_weight="1"
                        android:background="@drawable/btn_gray"
                        android:text="@string/ok"
                        android:textColor="#FFFFFF"
                        android:textSize="16sp" />
                    <Button
                        android:id="@+id/dialog_cancel"
                        android:layout_width="20dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="20dp"
                        android:layout_weight="1"
                        android:background="@drawable/corners_bg3"
                        android:text="@string/cancel"
                        android:textColor="#FFFFFF"
                        android:textSize="16sp" />
                </LinearLayout>
                <CheckBox
                    android:id="@+id/not_shown"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="#191970"
                    android:layout_gravity="right"
                    android:layout_marginRight="15dp"
                    android:layout_marginBottom="5dp"
                     android:textSize="14sp" 
                    android:text="@string/not_ask" />
            </LinearLayout>
    </RelativeLayout>
</RelativeLayout>
