<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        	 android:layout_width="match_parent"
        	 android:layout_height="match_parent"
        	 android:layout_gravity="center" >
        <TableRow
            android:id="@+id/tableRow1"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="50dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
        <CheckBox
        	android:id="@+id/Physical"
        	android:layout_weight="1"
        	android:layout_width="0dp"
        	android:layout_height="match_parent"
        	android:text="@string/Physical"
        	android:paddingLeft="12dp"
        	android:checked="true"
        	android:button="@drawable/checked_active"/>
    	<CheckBox
        	android:id="@+id/Transducer"
        	android:layout_weight="1"
        	android:layout_width="0dp"
        	android:layout_height="match_parent"
        	android:text="@string/Transducer"
        	android:paddingLeft="12dp"
        	android:checked="true"
        	android:button="@drawable/checked_active" />
    	<CheckBox
        	android:id="@+id/Point"
        	android:layout_weight="1"
        	android:layout_width="0dp"
        	android:layout_height="match_parent"
        	android:text="@string/Point"
        	android:paddingLeft="12dp"
        	android:checked="true"
        	android:button="@drawable/checked_active"/>
    	<CheckBox
        	android:id="@+id/Direction"
        	android:layout_weight="1"
        	android:layout_width="0dp"
        	android:layout_height="match_parent"
        	android:text="@string/Direction"
        	android:paddingLeft="12dp"
        	android:checked="true"
        	android:button="@drawable/checked_active"/>
        </TableRow>
       <View
        	android:id="@+id/line1"
       		android:layout_width="match_parent"
        	android:layout_height="0.5dp"
        	android:background="#D0D0D0"/>
        <TableRow
            android:id="@+id/tableRow2"
            android:layout_weight="1"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="50dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
    	<CheckBox
        	android:id="@+id/Coupling"
        	android:layout_weight="1"
        	android:layout_width="0dp"
        	android:layout_height="match_parent"
        	android:text="@string/Coupling"
        	android:paddingLeft="12dp"
        	android:checked="true"
        	android:button="@drawable/checked_active"/>
    	<CheckBox
        	android:id="@+id/Sensitivity"
        	android:layout_weight="1"
        	android:layout_width="0dp"
        	android:layout_height="match_parent"
        	android:text="@string/Sensitivity"
        	android:paddingLeft="12dp"
        	android:checked="true"
        	android:button="@drawable/checked_active"/>

    	<CheckBox
        	android:id="@+id/Range"
        	android:layout_weight="1"
        	android:layout_width="0dp"
        	android:layout_height="match_parent"
        	android:text="@string/Range"
        	android:paddingLeft="12dp"
        	android:checked="true"
        	android:button="@drawable/checked_active"/>

    	<TextView
    	    android:id="@+id/textView1"
    	    android:layout_weight="1"
    	    android:layout_width="match_parent"
    	    android:layout_height="match_parent"/>
        </TableRow>
        <View
        	android:id="@+id/line2"
       		android:layout_width="match_parent"
        	android:layout_height="0.5dp"
        	android:background="#D0D0D0"/>
        <TableRow
            android:id="@+id/tableRow3"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
    	<TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"/>
        <Button 
            android:id="@+id/Save"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:background="?attr/copy"
            android:textColor="#FFFEFF"
            android:layout_weight="1"
            android:layout_marginRight="10dp"
            android:layout_gravity="center"
            android:text="@string/Save"/>
        <Button 
            android:id="@+id/Restore_default"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:background="?attr/paste"
            android:textColor="#FFFEFF"
            android:text="@string/Recover"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_gravity="center"/>
        </TableRow>
</TableLayout>