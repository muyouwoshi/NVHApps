<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/auto_range"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentTop="true"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/textview_autorange"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="?attr/TabUpLayout"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/title_autorange"
            android:layout_width="wrap_content"
            android:layout_marginLeft="8dp"
            android:textSize="16dp"
            android:textColor="#FFFEFF"
            android:layout_alignParentLeft="true"
        	android:layout_centerVertical="true"
            android:layout_height="wrap_content"
            android:text="@string/title_autorange" />
         <Button
	       android:id="@+id/auto_range_return_bt"
	       android:layout_width="50dp"
	       android:layout_height="40dp"
	       android:layout_alignParentRight="true"
	       android:background="@drawable/back_button" />
    </RelativeLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/drawpages_autorange"
        android:background="@color/Type4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </android.support.v4.view.ViewPager>
<!-- android:background="@color/white" -->
    <LinearLayout
        android:id="@+id/function_autorange"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:gravity="center"
        android:background="#FFFFFF"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/recall_autorange"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:textColor="#FFFEFF"
            android:layout_marginRight="15dp"
            android:background="?attr/copy"
            android:text="@string/recall_autorange" />

        <Button
            android:id="@+id/maxdata_autorange"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:textColor="#FFFEFF"
            android:layout_marginRight="15dp"
            android:background="?attr/paste"
            android:text="@string/maxdata_autorange" />

        <Button
            android:id="@+id/addgear_autorange"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:textColor="?attr/textColor"
            android:background="?attr/check_all"
            android:text="@string/addgear_autorange" />
    </LinearLayout>

</LinearLayout>